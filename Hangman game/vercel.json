{
  "version": 2,
  "builds": [
    {
      "src": "main.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "script/*",
      "use": "@vercel/static"
    },
    {
      "src": "image/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/main.html" }
  ]
}
